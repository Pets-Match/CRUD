generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Owner {
  id    Int    @id @default(autoincrement())
  name  String
  email String @unique
  password String
  address Address @relation(fields: [addressId], references: [id])
  addressId Int
  doc String
  phone String
}

model Address {
  id Int @id @default(autoincrement())
  owners Owner[]
  zipCode String
  state String
  city String
  neighborhood String
  street String
  num String
  addInfo String
}
